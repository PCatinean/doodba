<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="filter_open_claims" model="ir.filters">
            <field name="name">Open Claims</field>
            <field name="model_id">crm.claim</field>
            <field name="domain" eval="[('stage_id.close', '=', False)]" />
            <field name="user_id" eval="False" />
            <field name="is_default" eval="True" />
            <field name="context">{}</field>
        </record>
    </data>

    <data noupdate="1">
        <record model="crm.claim.stage" id="crm_claim.stage_claim1">
            <field name="close" eval="False" />
        </record>
        <record model="crm.claim.stage" id="crm_claim.stage_claim5">
            <field name="close" eval="False" />
        </record>
        <record model="crm.claim.stage" id="crm_claim.stage_claim2">
            <field name="close" eval="True" />
        </record>
        <record model="crm.claim.stage" id="crm_claim.stage_claim3">
            <field name="close" eval="True" />
        </record>
    </data>
</openerp>
